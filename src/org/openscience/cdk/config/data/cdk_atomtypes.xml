<atomTypeList xmlns="http://www.xml-cml.org/schema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.xml-cml.org/schema ../../io/cml/data/cml25b1.xsd"

  xmlns:cvs="https://www.cvshome.org/"
  xmlns:cdk="http://cdk.sf.net/dict/cdk/"

  id="cdk" title="CDK AtomTypes">

  <!-- DRAFT atom type list to replace all all current lists -->

  <metadataList>
    <metadata name="cvs:last-change-by" content="$Author: egonw $"/>
    <metadata name="cvs:date" content="$Date: 2007-07-17 08:28:56 +0200 (Tue, 17 Jul 2007) $"/>
    <metadata name="cvs:revision" content="$Revision: 8549 $"/>
  </metadataList>
  
  <atomType id="C.sp2">
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:lonePairCount">0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:piBondCount">1</scalar>
      <!-- max bond order = piBC + 1 -->
      <!-- bond order sum = fNC + piBc -->
      <!-- # hydrogens = fNC - actual Neighbor Count -->
    </atom>
    <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
  </atomType>

</atomTypeList>
